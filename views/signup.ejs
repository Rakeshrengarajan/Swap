    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>

    <!-- Popper JS -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.16.0/umd/popper.min.js"></script>

    <!-- Latest compiled JavaScript -->
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js"></script>

    <!--Font-->
    <link rel="preconnect" href="https://fonts.gstatic.com">
    <link href="https://fonts.googleapis.com/css2?family=Poppins&display=swap" rel="stylesheet">

    <!--Icons-->
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.7.0/css/all.css" integrity="sha384-lZN37f5QGtY3VHgisS14W3ExzMWZxybE1SJSEsQp9S+oqd12jhcu+A56Ebc1zFSJ" crossorigin="anonymous">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">

    <link rel="icon" href="design/images/swap-logo-light.ico" type="icon type">

    <!--Styling-->
    <link rel="stylesheet" type="text/css" href="design/css/style.css">

</head>

<body class="d-flex flex-column min-vh-100">

    <section>
        <div class="container-fluid">
            <!--Navigation bar-->

            <nav class="navbar fixed-top navbar-expand-md navbar-dark fixed-top">
                <div class="container">

                    <!--Brand-->
                    <a class="navbar-brand" href="/">&nbsp;<img src="design/images/swap-dark.png" class="brand"></a>

                    <!--To display an icon in mobile view-->
                    <button class="navbar-toggler animated-button" type="button" data-toggle="collapse" data-target="#navbar" aria-controls="navbar" aria-expanded="false" aria-label="Toggle navigation">
                        <div class="animated-icon"><span></span><span></span><span></span></div>
                    </button>

                    <!--Links-->
                    <div class="collapse navbar-collapse" id="navbar">

                        <ul class="navbar-nav ml-auto">
                            <li class="nav-item">
                                <a class="nav-link" href="/">About</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" href="team">Team</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" href="training">Trainings</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" href="webinar">Webinars</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" href="contact">Contact</a>
                            </li>
                        </ul>
                    </div>
                </div>
            </nav>
        </div>
    </section>

    <!--End of Navigation bar-->

    <br><br>

    <section id="signup">
        <div class="container-fluid">
            <div class="row">
                <div class="col-md-6">
                    <center>
                        <br><br>
                        <img src="design/images/signup.png" class="img-responsive signup">
                    </center>
                </div>
                <div class="col-md-6">
                    <center>
                        <br><br>
                        <form class="was-validated" action="/accdetails" method="post">

                            <div class="form-group">
                                <label for="name">Name:</label>
                                <input type="text" class="form-control" placeholder="Enter name" name="name" required>
                                <div class="valid-feedback">Valid.</div>
                                <div class="invalid-feedback">Please fill out this field.</div>
                            </div>

                            <div class="form-group">
                                <label for="email">Email:</label>
                                <input type="email" class="form-control" placeholder="Enter email" name="email" required>
                                <div class="valid-feedback">Valid.</div>
                                <div class="invalid-feedback">Please fill out this field.</div>
                            </div>

                            <div class="form-group">
                                <label for="phone">Phone Number:</label>
                                <input type="tel" class="form-control" placeholder="Enter phone number" name="phone" required>
                                <div class="valid-feedback">Valid.</div>
                                <div class="invalid-feedback">Please fill out this field.</div>
                            </div>

                            <div class="form-group">
                                <label for="whatsapp">WhatsApp Number:</label>
                                <input type="tel" class="form-control" placeholder="Enter WhatsApp number" name="wphone" required>
                                <div class="valid-feedback">Valid.</div>
                                <div class="invalid-feedback">Please fill out this field.</div>
                            </div>

                            <div class="form-group">
                                <label for="college-name">College Name:</label>
                                <input type="text" class="form-control" placeholder="Enter college name" name="clgname" required>
                                <div class="valid-feedback">Valid.</div>
                                <div class="invalid-feedback">Please fill out this field.</div>
                            </div>

                            <div class="form-group">
                                <label for="department">Department:</label>
                                <input type="text" class="form-control" placeholder="Enter Department" name="deptname" required>
                                <div class="valid-feedback">Valid.</div>
                                <div class="invalid-feedback">Please fill out this field.</div>
                            </div>

                            <div class="form-group">
                                <label for="Batch">Batch:</label>
                                <input type="text" class="form-control" placeholder="Enter Batch eg:2018-2022" name="batch" required>
                                <div class="valid-feedback">Valid.</div>
                                <div class="invalid-feedback">Please fill out this field.</div>
                            </div>


                            <div class="form-group">
                                <label for="password">Password:</label>
                                <input type="password" class="form-control" placeholder="Enter password" name="password" required>
                                <div class="valid-feedback">Valid.</div>
                                <div class="invalid-feedback">Please fill out this field.</div>
                            </div>

                            <% if(alertbox == 1){ %>

                            <div class="alert alert-danger">
                                <strong>User</strong> Already Exists.
                            </div>

                            <%  }else{ %>

                            <br>

                            <% } %>


                            <center><button type="submit" class="btn btn-danger">Create Account</button></center>
                            <br>
                            <center><a href="/acclogin" class="login-ask">Already have an account?</a></center>

                        </form>
                    </center>
                </div>
            </div>
        </div>
    </section>

    <script src="design/js/block.js"></script>
    <script src="design/js/navbar.js"></script>
    
       <section id="forum">
        <div class="container-fluid">
            <center>
                <p>For Internships and placements</p>
                <a href="https://t.me/internshipsandplacements"><button class="btn btn-outline-dark"><i class="fab fa-telegram-plane" style="font-size: 24px;color:33BBFF;">&nbsp;</i>Join our Forum</button></a>
            </center>
        </div>
    </section>
    <br>
    
    <footer class="footer mt-auto py-3" style="background-color: #000000;">
        <div class="container text-center">
        <a href=" https://instagram.com/swapincofficial?igshid=13lx5d5x4b86i"><i class="fab fa-instagram" style="font-size:30px;color:crimson;"></i></a>&nbsp;&nbsp;&nbsp;&nbsp;
        <a href="https://www.linkedin.com/company/swapinc"><i class="fa fa-linkedin" style="font-size:30px;color:rgb(0, 119, 181)"></i></a>&nbsp;&nbsp;&nbsp;&nbsp;
        <a href="https://swapincvec.medium.com"><i class="fab fa-medium" style="font-size:30px;color:white;"></i></a>
        </div>
    </footer>



</body>

</html>
